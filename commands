- RUN UNIT TESTS
    mvn test -B -Dspring.profiles.active=${SPRING_PROFILES_ACTIVE}
- RUN INTEGRATION TESTS
    mvn verify -B -Dspring.profiles.active=${SPRING_PROFILES_ACTIVE}
- RUN MAVEN PACKAGE (MODULE)
    mvn clean package -B -pl hotel -am -DskipTests
- RUN APPLICATION
    java -jar -Dspring.profiles.active=${SPRING_PROFILES_ACTIVE} hotel\target\hotel-0.0.1-SNAPSHOT.jar
- BUILD DOCKER IMAGE
    docker build -t hotel -f hotel\Dockerfile || hotel/Dockerfile .
- RUN DOCKER IMAGE
    docker run --rm --name hotel -p 8080:8080 --env-file master.env -it hotel